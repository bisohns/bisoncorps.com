{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
  "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
    {
      "extensions": [ ".js", ".jsx" ]
    }
    ],
    "prettier/prettier": [
      "error",
    {
      "semi": false
    }
    ],
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/require-default-props":0,
    "react/forbid-prop-types":0,

  }
}
